{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:client:analyze": "ANALYZE=true vite build",
    "build:client:optimized": "NODE_ENV=production vite build --mode production",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "analyze": "npm run build:client:analyze && open dist/bundle-analysis.html"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
